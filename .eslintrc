{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb/base", "plugin:protractor/recommended"],
  "plugins": ["protractor"],
  "rules": {
    "no-await-in-loop": "off",
    "class-methods-use-this": "off",
    "no-plusplus": "off",
    "eol-last": "off",
    "max-len": [
      "error",
      150,
      {
        "ignoreUrls": true
      }
    ],
    "no-tabs": "off",
    "no-alert": "off",
    "prefer-destructuring": "off",
    "indent": [2, "tab"],
    "no-param-reassign": "off",
    "linebreak-style": 0,
    "new-cap": "off",
    "consistent-return": "off"
  }
}
